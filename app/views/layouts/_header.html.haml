.navbar-inner.clearfix
  = link_to image_tag("white logo.png", :border=>0), '#', :class => 'brand'
  %ul.nav
    %li.dropdown
      %a{:href => "#", "data-toggle" => "dropdown"} San Francisco <span class="caret color=white"></span>
      %ul.dropdown-menu{"aria-labeledby" => "dLabel", :role => "menu"}
        %li.dropitem
          %a{:href => "#", :tabindex => "-1"} Other City 1
        %li.dropitem
          %a{:href => "#", :tabindex => "-1"} Other City 2
        %li.divider
        %li.dropitem 
          %p Don't see your city? Enter your zip code or city name
          %form.navbar-form
            %input.span2{:type => "text"}/
    %li.pull-right
      %a{"data-toggle" => "modal", :href => "#sign_up", :role => "button"} Sign Up
    %li.pull-right
      -if user_signed_in?
        = link_to "Sign Out", destroy_user_session_path, :method => :delete, :confirm => "Are you sure you want to sign out?"
      -if user_signed_in? == false
        = link_to "Log In", new_user_session_path
    %li.dropdown.pull-right
      %a{:href => "#", "data-toggle" => "dropdown"} About <span class="caret color=white"></span>
      %ul.dropdown-menu{"aria-labeledby" => "dLabel", :role => "menu"}
        %li.dropitem
          %a{:href => "#", :tabindex => "-1"} About
        %li.dropitem
          %a{:href => "#", :tabindex => "-1"} Jobs
        %li.dropitem
          %a{:href => "#", :tabindex => "-1"} Legal
        %li.dropitem
          %a{:href => "#", :tabindex => "-1"} Contact Us 
    %li.pull-right
      %a{:href => "#"} Create an Event